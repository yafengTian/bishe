<!-- banner -->
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" data-src="{{item}}" bindtap="previewImage"></image>
    </swiper-item>
  </block>
</swiper>
<scroll-view scroll-y="true">
  <view class="detail">
  <view wx:for="{{details}}">
  <text class="price">￥{{item.price}} {{item.descration}}</text>
   <view class='line'></view>
  <view class='center'>景区介绍</view>
   <view class='line'></view>
  <view class='content'>{{item.introducation}}</view>
  </view>
  </view>
  <view class='line'></view>
  <view class='center'>景区一角</view>
  <view class='line'></view>
  <block wx:for-items="{{detailImg}}" wx:key="name">
    <image class="image_detail" src="{{item}}" />
  </block>
</scroll-view>
<!-- 底部悬浮栏 -->
<view class="detail-nav">
  <image bindtap="toCar" src="../../resources/images/ic_listing_selected.png" />
  <view class="line_nav"></view>
  <image bindtap="addLike" src="{{isLike?'../../resources/images/unshoucang.png':'../../resources/images/shoucang.png'}}" />
  <button class="button-green" bindtap="mylist" formType="submit">我的订单</button>
  <button class="button-red" bindtap="immeBuy" formType="submit">去下单</button>
</view>