<!-- banner -->
<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" data-src="{{item}}" bindtap="previewImage"></image>
    </swiper-item>
  </block>
</swiper>
<scroll-view scroll-y="true">
  <view class="detail">
  <block wx:for="{{details}}">
  <text class="price">原价￥200元 现在只要￥{{item.price}}元</text>
   <text class="title">{{item.descration}}</text>
  <text>景区详情</text>
  <text>{{item.introducation}}</text>
  </block>
  </view>
  <block wx:for-items="{{detailImg}}" wx:key="name">
    <image class="image_detail" src="{{item}}" />
  </block>
  <view class="temp"></view>
</scroll-view>
<!-- 底部悬浮栏 -->
<view class="detail-nav">
  <image bindtap="toCar" src="../../resources/images/ic_listing_selected.png" />
  <view class="line_nav"></view>
  <image bindtap="addLike" src="{{isLike?'../../resources/images/unshoucang.png':'../../resources/images/shoucang.png'}}" />
  <button class="button-green" bindtap="addCar" formType="submit">加入购物车</button>
  <button class="button-red" bindtap="immeBuy" formType="submit">立即购买</button>
</view>